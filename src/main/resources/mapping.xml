<?xml version="1.0" encoding="utf-8"?>
<mapping>

	<!-- Corresponding to the CMIS property ID "cmis:objectTypeId" -->
	<objectTypes>
		<objectType id="series">delta:series</objectType>
		<objectType id="subseries">delta:subseries</objectType>
		<objectType id="record">delta:record</objectType>
	</objectTypes>

	<eadTemplates>
		<template id="series">
			<c01 level="series">
				<did>
					<unitdate datechar="created">
						<cmisProperty id="cmis:creationDate" />
					</unitdate>
					<unittitle>
						<cmisProperty id="cmis:name" />
					</unittitle>
					<unitid localtype="system_id">
						<cmisProperty id="cmis:objectId" />
					</unitid>
				</did>
			</c01>
		</template>
		<template id="subseries">
			<c02 level="subseries">
				<did>
					<unitdate datechar="created">
						<cmisProperty id="cmis:creationDate" />
					</unitdate>
					<unittitle>
						<cmisProperty id="cmis:name" />
					</unittitle>
					<unitid localtype="system_id">
						<cmisProperty id="cmis:objectId" />
					</unitid>
				</did>
			</c02>
		</template>
		<template id="record">
			<c03 level="file">
				<did>
					<unitdate datechar="created">
						<cmisProperty id="cmis:creationDate" />
					</unitdate>
					<unittitle>
						<cmisProperty id="cmis:name" />
					</unittitle>
					<unitid localtype="system_id">
						<cmisProperty id="cmis:objectId" />
					</unitid>
					<daoset label="Digital Object" coverage="whole">
						<!-- dao elements are placed here at a later stage -->
					</daoset>
				</did>
			</c03>
		</template>
	</eadTemplates>

</mapping>
